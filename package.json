{
  "name": "connec",
  "version": "1.0.0",
  "description": "Uma aplicacao front end para o projeto do am de TDS",
  "main": "connec-index.js",
  "scripts": {
    "preinstall": "npx use-yarn",
    "commit": "git-cz",
    "start": "npm run clear && npm run server",
    "prod": "npm run clear && npm run linters && npm run build",
    "lint:js": "eslint ./src --ext .js,.jsx --color",
    "lint:js:fix": "yarn lint:js --fix --color",
    "pug:lint": "pug-lint ./src",
    "linters": "yarn lint:js && yarn pug:lint",
    "server": "cross-env NODE_ENV=development && webpack-dev-server --watch --colors --open --config webpack.dev.js --env.ENVIROMENT=development",
    "build": "webpack -m -p --colors --config webpack.prod.js --env.ENVIROMENT=production",
    "clear": "rimraf ./dist"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/LeandroFariasLourenco/connec.git"
  },
  "keywords": [
    "js",
    "react"
  ],
  "author": "Leandro Farias Lourenco",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/LeandroFariasLourenco/connec/issues"
  },
  "homepage": "https://github.com/LeandroFariasLourenco/connec#readme",
