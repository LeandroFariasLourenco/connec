{
  "name": "connec",
  "version": "1.0.0",
  "description": "Uma aplicacao front end para o projeto do am de TDS",
  "main": "connec-index.js",
  "scripts": {
    "start": "npm run build && webpack-dev-server --mode development --watch --colors --open",
    "build": "webpack --mode production --colors",
    "lint:js": "eslint ./src --ext .js,.jsx --color",
    "lint:js:fix": "npm run lint:js --fix --color",
    "pug:lint": "pug-lint ./src",
    "linters": "npm run lint:js && npm run pug:lint",
    "remove:dist": "rm -rf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/LeandroFariasLourenco/connec.git"
  },
  "keywords": [
    "js",
    "react"
  ],
  "author": "Leandro Farias Lourenco",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/LeandroFariasLourenco/connec/issues"
  },
  "homepage": "https://github.com/LeandroFariasLourenco/connec#readme",
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run linters",
      "pre-push": "npm run linters",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.2.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-watch": "^7.0.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^3.1.0",
    "husky": "^4.2.5",
    "image-webpack-loader": "^6.0.0",
    "lint-staged": "^10.2.10",
    "pug": "^2.0.0",
    "pug-lint-config-clock": "^2.0.0",
    "pug-loader": "^2.4.0",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.1.1",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-node-externals": "^1.7.2",
    "yarn": "^1.22.4"
  },
  "engines": {
    "node": ">= 11.14.0",
    "npm": ">=6.7.0"
  }
}
